      "cell_type": "code",
      "metadata": {
        "id": "Z8rOcDFviTVp",
        "colab_type": "code",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 353
        },
        "outputId": "3ca1c7d7-fa82-453e-e165-07902ba51653"
      },
      "source": [
        "# f(x) = a * x ** 2 + b * x + c\n",
        "def f(x, a=1.0, b=-2.0, c=1.0):\n",
        "  return a * torch.pow(x, 2) + b * x + c\n",
        "\n",
        "x = torch.tensor(10.0, requires_grad=True)\n",
        "optimizer = torch.optim.SGD(params=[x], lr=0.1)\n",
        "\n",
        "for i in range(1, 101):\n",
        "  optimizer.zero_grad()\n",
        "  y = f(x)\n",
        "  y.backward()\n",
        "  optimizer.step()\n",
        "  if i % 5 == 0:\n",
        "    print(f'iter {i} : f(x) = {y.data:.6f}, x = {x.data:.6f}')"
      ],
